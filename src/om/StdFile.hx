package om;

/**
	Input and output communication channels between a computer program and its environment when it begins execution.
**/
enum StdFile {
	/**
		Standard input stream (`stdin`).
	**/
	StdIn;

	/**
		Standard output stream (`stdout`).
	**/
	StdOut;

	/**
		Standard error stream (`stderr`).
	**/
	StdErr;
}
